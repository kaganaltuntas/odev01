sozluk = {"LOL":"komik bir şeye verilen cevap","CRINGE":"garip ya da utandırıcı bir şey","ROFL":"bir şakaya karşılık cevap","SHEESH":"onaylamamak","CREEPY":"korkunç","AGGRO":"agresifleşmek/sinirlenmek"}
a = input("merhaba anlamadığınız kelimeyi lütfen bütün harfleri büyük olacak şekilde yazınız:")
if a in sozluk.keys():
  print(sozluk[a])
else:
    print("üzgünüm bu kelime sözlükte bulunmuyor ama üstünde çalışıyoruz.")
